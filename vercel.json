{
  "version": 2,
  "builds": [
      {
          "src": "build/index.js",
          "use": "@vercel/node",
          "config": { "includeFiles": ["build/**"] }
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "pages": "build/pages",
          "dest": "build/pages/index.js"
      }
  ]
}